{
    "name": "kex/kex-config",
    "type": "library",
    "description": "An object oriented configuration library",
    "keywords": [ "config" ],
    "homepage": "https://github.com/anthonyhowell/KexConfig",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Howell",
            "email": "anthonyhowell@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "pimple/pimple": "~1.0",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "symfony/routing": "~2.3|~3.0",
        "silex/silex": "~1.0",
        "twig/twig": "~1.16|~2.0"
    },
    "suggest": {
        "pimple/pimple": "for the built-in implementations of the menu provider and renderer provider",
        "silex/silex": "for the integration with your silex application",
        "twig/twig": "for the TwigRenderer and the integration with your templates"
    },
    "autoload": {
        "psr-4": {
            "Kex\\Config\\": "src/Kex/Config"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kex\\Config\\Test\\": "tests/Kex/Config/Test"
        }
    }
}